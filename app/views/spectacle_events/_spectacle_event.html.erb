<%# collection_row_cache(spectacle_event) do %>
	<li>
		  <span class="dates"><%= format_spectacle_event_date(spectacle_event) %></span>
      <strong class="title">
        <% if spectacle_event.spectacle %>
          <%= link_to(spectacle_event.spectacle.title,     
                        spectacle_event.spectacle.public_url) %>
        <% else %>
          <%= link_to("Formation", "#") %>
        <% end %>
      </strong>
     	<% if spectacle_event.location %>
      	<span class="location"><%= spectacle_event.location %></span>
     	<% end %>
     	<% if spectacle_event.place %>
       	<span class="place">
        <%= spectacle_event.more_info_url.present? ? link_to(spectacle_event.place, spectacle_event.more_info_url, :target => :blank) : spectacle_event.place %>
		</span>
		<% end %>
	</li>
<%# end if spectacle_event %>